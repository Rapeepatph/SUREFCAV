<div class="container">
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <div class="col-md-12 text-center">
            <h4>SUREF</h4>
        </div>

        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner">
            <div class="item active">
                <img src="~/img/plane-1448300.jpg" alt="Los Angeles" style="width:100%;">
            </div>

            <div class="item">
                <img src="~/img/aircraft-123005.jpg" alt="Chicago" style="width:100%;">
            </div>

            <div class="item">
                <img src="~/img/contrail-670296.jpg" alt="New york" style="width:100%;">
            </div>
        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>
 <div class="container" ng-controller="summaryController">
        <input type="hidden" id="Date" value="@ViewBag.RecentlyDate" />
        <hr />
        <table class="table table-bordered" ng-init="landingCall()">
            <thead>
                <tr>
                    <th>Probability of update on lastest day </th>
                    <th>@Html.ActionLink("SSR/MRT", "Index", "Summary", routeValues: new { selectedDate = ViewBag.RecentlyDate, sensorid = 2 }, htmlAttributes: null) </th>
                    <th>@Html.ActionLink("ADS-B", "Index", "Summary", routeValues: new { selectedDate = ViewBag.RecentlyDate, sensorid = 1 }, htmlAttributes: null) </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <th>Horizontal Position</th>
                    <td>{{R2_Land_62}} %</td>
                    <td>{{R2_Land_21 }} %</td>
                </tr>
                <tr>
                    <th>Altitude</th>
                    <td>{{R7_Land_62}} %</td>
                    <td>{{R7_Land_21}} %</td>
                </tr>
                <tr>
                    <th>Identification</th>
                    <td>{{R14_Land_62}} %</td>
                    <td>{{R14_Land_21}} %</td>
                </tr>
            </tbody>
        </table>
        <p>Information from Data on {{dateInfo}}</p>
 </div>
